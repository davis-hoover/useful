[grid]
s0_put_left_key = <Super>Left
s0_put_right_key = <Super>Right
s0_put_topleft_key = <Shift><Super>Left
s0_put_topright_key = <Shift><Super>Right
s0_put_bottomleft_key = <Control><Super>Left
s0_put_bottomright_key = <Control><Super>Right
s0_put_maximize_key = <Super>Up
s0_put_restore_key = <Super>Down
s0_left_maximize = Disabled
s0_right_maximize = Disabled

[composite]
s0_refresh_rate = 50

[unityshell]
s0_shortcut_overlay = false
s0_inactive_shadow_color = #000000a5
s0_show_launcher = <Super>space
s0_launcher_hide_mode = 1

[gnomecompat]
s0_main_menu_key = <Super>s
s0_command_window_screenshot = gnome-screenshot -w
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Control><Alt>t

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[expo]
s0_distance = 0.005000
s0_vp_saturation = 40.000000
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[animation]
s0_unminimize_effects = animation:Glide 2;

[core]
s0_active_plugins = core;composite;opengl;wall;regex;grid;move;snap;commands;compiztoolbox;imgpng;place;vpswitch;mousepoll;copytex;resize;unitymtgrabhandles;animation;session;fade;workarounds;expo;scale;ezoom;unityshell;
s0_outputs = 1920x1080+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_unmaximize_or_minimize_window_key = <Super>Down
s0_show_desktop_key = <Control><Super>d

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9

